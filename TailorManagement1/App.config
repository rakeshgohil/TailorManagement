<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="languages" type="System.Configuration.NameValueSectionHandler" />
	</configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
    </startup>
	<appSettings>
		<add key="BaseUrl" value="http://localhost/TailorManagementAPI/" />
	</appSettings>
	<languages>
		<add key="en-US" value="English" />
		<add key="hi-IN" value="Hindi" />
		<add key="gu-IN" value="Gujarati" />
	</languages>
</configuration>